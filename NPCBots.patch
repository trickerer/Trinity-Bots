+    //*etStats() has *pplyClassPassives() in it
+    //needed to be before InitEquips for some classes (warrior TG)
+    me->SetPvP(true);
+    InitRoles();
+    SetStats(true);
+    //InitPowers(); //already in *etStats();
+    ApplyPassives();
+
+    if (firstspawn)
+    {
+        firstspawn = false;
+        ASSERT(!me->GetBotAI());
+        me->SetBotAI(this);
+        InitFaction();
+        InitOwner();
+        InitEquips();
+
+        InitSpellMap(PVPTRINKET, true);
+    }
+}
